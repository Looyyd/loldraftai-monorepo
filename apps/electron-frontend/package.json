{
  "name": "electron-frontend",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "scripts": {
    "clean": "rimraf dist renderer/out renderer/.next",
    "dev": "yarn clean && yarn build-electron && concurrently \"yarn dev:renderer\" \"yarn dev:main\" \"yarn dev:electron\"",
    "dev:renderer": "next dev renderer",
    "dev:main": "tsc -p tsconfig.electron.json -w",
    "dev:electron": "wait-on tcp:3000 && electron .",
    "build": "yarn build-renderer && yarn build-electron",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p tsconfig.electron.json",
    "pack-app": "yarn build && electron-builder --dir",
    "dist": "yarn build && electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  },
  "dependencies": {
    "electron": "^29.0.0",
    "node-fetch": "2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@types/js-cookie": "^3.0.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "heroicons": "^2.2.0",
    "install": "^0.13.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.461.0",
    "next": "15.0.3",
    "next-themes": "^0.4.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/node-fetch": "2",
    "concurrently": "^9.1.0",
    "electron-builder": "^24.0.0",
    "rimraf": "^6.0.1",
    "wait-on": "^8.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
